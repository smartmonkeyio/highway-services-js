{
  "name": "highway-services-js",
  "version": "0.0.1",
  "description": "This package allows the users to easily integrate SmartMonkey services in their code.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha 'test/**/*.ts'",
    "build": "npm run clean  && tsc",
    "clean": "rm -rf .coverage .nyc_output dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:smartmonkeyio/highway-services-js.git"
  },
  "keywords": [
    "api",
    "smartmonkey",
    "services"
  ],
  "author": "smartmonkey",
  "license": "MIT",
  "homepage": "",
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "lib/**/*.tsx"
    ],
    "exclude": [
      "test/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/joi": "^14.3.1",
    "@types/mocha": "^5.2.7",
    "@types/request-promise": "^4.1.42",
    "dotenv": "^7.0.0",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "joi": "^14.3.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "files": [
    "dist/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
