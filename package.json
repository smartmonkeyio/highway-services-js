{
  "name": "highway-api",
  "version": "1.0.1",
  "description": "This package allows the users to easily integrate Highway services in their code.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "http://apidocs.smartmonkey.io/",
  "scripts": {
    "test": "nyc mocha 'test/**/*.ts'",
    "build": "npm run clean  && tsc",
    "clean": "rm -rf .coverage .nyc_output dist",
    "prepare": "yarn run build",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:smartmonkeyio/highway-services-js.git"
  },
  "keywords": [
    "api",
    "smartmonkey",
    "services"
  ],
  "author": {
    "name": "Pablo Mart√≠nez",
    "email": "pablo.martinez@smartmonkey.io",
    "url": "https://github.com/pablomartinezm"
  },
  "license": "MIT",
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "lib/**/*.tsx"
    ],
    "exclude": [
      "test/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/joi": "^14.3.1",
    "@types/mocha": "^5.2.7",
    "@types/request-promise": "^4.1.42",
    "dotenv": "^8.2.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "@hapi/joi": "^17.1.0"
  },
  "files": [
    "dist/**/*"
  ]
}